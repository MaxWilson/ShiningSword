{
  "private": true,
  "scripts": {
    "start": "dotnet tool restore && dotnet fable watch src --runFast vite --port 8080",
    "buildGithub": "dotnet fable src --sourceMaps --run vite build --base /POCArena",        
    "build": "dotnet tool restore && dotnet fable src --run vite build",
    "clean": "dotnet fable clean src --yes"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^3.1.0",
    "vite": "^4.1.0"
  },
  "engines": {
    "node": ">=18"
  }
}
